{
  "name": "@core-api/ums",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "server:build": "nest build",
    "server:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "server:start": "nest start",
    "server:dev": "nest start --watch",
    "server:start:debug": "nest start --debug --watch",
    "server:start:prod": "node dist/main",
    "server:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "server:test": "jest",
    "server:test:watch": "jest --watch",
    "server:test:cov": "jest --coverage",
    "server:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "jest --config ./test/jest-e2e.json",
    "server:cmd_bootstrap": "ts-node  -r tsconfig-paths/register --transpile-only job/squad_point/bootstrap.ts",
    "typeorm": "node --require ts-node/register --require tsconfig-paths/register ./node_modules/@core-api/nest-typeorm-postgres/node_modules/typeorm/cli.js -d src/database/dbConnectOptions.ts",
    "migration:create": "typeorm migration:create src/migrations/generates/dataseed -t",
    "migration:generate": "npm run typeorm migration:generate src/migrations/generates/dataseed -t",
    "migration:run": "npm run typeorm migration:run",
    "docker": "docker-compose up -d"
  },
  "dependencies": {
    "@core-api/nest-core": "workspace:^",
    "@core-api/nest-typeorm-postgres": "workspace:^",
    "@core-config/eslint-plugin": "workspace:^",
    "@core-config/release": "workspace:^",
    "@core-config/tsconfig": "workspace:^",
    "@core-config/typedoc": "workspace:^",
    "@core-modules/permissions": "workspace:^",
    "@core-modules/roles": "workspace:^",
    "@core-modules/users": "workspace:^",
    "@core-ui/ums-types": "workspace:^",
    "@core-ui/recruiter-types": "workspace:^",
    "@core-api/microservices-utils": "workspace:^",
    "@core-api/appcenter-materials": "workspace:^",
    "@orbs-network/ton-access": "^2.3.3",
    "@ton/core": "^0.56.3",
    "@ton/ton": "^14.0.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.1",
    "jsdom": "^25.0.0",
    "nestjs-telegraf": "^2.8.1",
    "pg": "^8.12.0",
    "rxjs": "^7.8.1",
    "telegraf": "^4.16.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cheerio": "^0.22.35",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.12.10",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "csv-parser": "^3.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^1.13.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
